{{#if posts.length}}
<section>
    <h2>Your Posts</h2>
    {{#each posts as |post|}}
    {{> dashboard-postinfo post}}
    {{/each}}
</section>
{{/if}}

<button type="submit" class="btn"><a class="link" href="/dashboard/new">Add New Post</a></button>

<script src="/public/js/add-post.js"></script>
<script src="/public/js/comment.js"></script>